{
  "extends": "ts-config-single-spa",
  "compilerOptions": {
    "jsx": "react-jsx",
    "declarationDir": "dist",
    "baseUrl": "./src",
    "paths": {
      "@Types/*": [
        "./@types/*"
      ],
      "@Assets/*": [
        "./assets/*"
      ],
      // "@Components/*": [
      //   "./components/"
      // ],
      "@Hooks/*": [
        "./hooks/*"
      ],
      "@Utils/*": [
        "./utils/*"
      ],
      "@Atoms/*": [
        "./components/atoms/*"
      ],
      "@Molecules/*": [
        "./components/molecules/*"
      ],
      "@Organisms/*": [
        "./components/organisms/*"
      ],
      "@Templates/*": [
        "./components/templates/*"
      ],
    }
  },
  "files": [
    // "src/raphaelbrodrigues-forwading-agent-landing.tsx"
  ],
  "include": [
    "src/**/*",
    "node_modules/@types"
  ],
  "exclude": [
    "src/**/*.test*"
  ]
}